#include <iostream>
#include<bits/stdc++.h>
using namespace std;

class minheap{
    private:
        int *arr;
        int size;
        int capacity;

    public:
        minheap(int c)
        {
            size=0;
            capacity=c;
            arr=new int[c];

        }
        int left(int i){ return 2*i + 1 ;}
        int right(int i){ return 2*i + 2 ;}
        int parent(int i){ return (i-1)/2 ;}
       void insertKey(int x){
           cout<<capacity<<size<<" "<<endl;
            if(size==capacity){
                return;
            }
            size++;
            int i=size-1;
            arr[i]=x;
         
            while(i!= 0 && arr[parent(i)] > arr[i]){
                swap(arr[i],arr[parent(i)]);
                i=parent(i);
            }
        }
        void show(){
            for(int i=0;i<size;i++){
                cout<<arr[i]<<" ";
            }
            cout<<endl;
        }
        
    
};

int main()
{
    minheap h(100);
    h.insertKey(12); 
    h.insertKey(2); 
    h.insertKey(4); 
    h.insertKey(5); 
    h.insertKey(14); 
    h.insertKey(45); 
    
    h.show();
   return 0;
}
